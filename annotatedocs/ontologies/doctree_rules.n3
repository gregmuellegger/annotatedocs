@prefix : <http://gremu.net/documentation-ontology/#> .


# If A contains B directly, then B hasParent A.
:hasParent a owl:FunctionalProperty ;
    rdfs:subPropertyOf :hasAncestor .

# If A contains B directly, then A hasChild B.
:hasChild owl:inverseOf :hasParent ;
    rdfs:subPropertyOf :hasDescendant .

# Transitive version of hasParent.
:hasAncestor a owl:TransitiveProperty .

# Transitive version of hasChild.
:hasDescendant a owl:TransitiveProperty .


# All text nodes that are inside a title element.
:TitleText owl:equivalentClass [
    a owl:Class ;
    owl:intersectionOf (
        :TextNode
        [
            a owl:Restriction ;
            owl:onProperty :hasAncestor ;
            owl:someValuesFrom :TitleElement
        ]
    )
] .


#
# The base class for all elements in a document.
#
:Element a owl:Class .

# Contains meta data about the documentation, like author, version, language, etc.
:MetaElement rdfs:subClassOf :Element .

# Contains content that is contains actual documentation.
:ContentElement rdfs:subClassOf :Element .

:ProseElement rdfs:subClassOf :ContentElement .

:TitleElement rdfs:subClassOf :ProseElement .

# Title elements.

:Title rdfs:subClassOf :TitleElement .
