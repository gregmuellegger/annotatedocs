@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix : <http://gremu.net/da/ontologies/2013/10/do178b/labels#> .


<http://gremu.net/da/ontologies/2013/10/do178b/labels#> 
  a owl:Ontology ;
  owl:imports <http://www.w3.org/2004/02/skos/core#> .


<urn:enhancement-2>
      a       <http://fise.iks-project.eu/ontology/EntityAnnotation> , <http://fise.iks-project.eu/ontology/Enhancement> ;
      <http://fise.iks-project.eu/ontology/entity-label>
              "software architecture"@en ;
      <http://fise.iks-project.eu/ontology/entity-reference>
              <http://gremu.net/da/ontologies/2013/10/do178b/labels#SoftwareArchitecture> ;
      <http://fise.iks-project.eu/ontology/extracted-from>
              <urn:content-item-sha1-2> .


:hasEnhancement a owl:ObjectProperty ;
    owl:inverseOf <http://fise.iks-project.eu/ontology/extracted-from> .

:SoftwareOverview a :PSACBaseEntity .
:SoftwareOverview skos:prefLabel "software overview"@en .

:Architecture skos:broader :SoftwareOverview .
:SoftwareArchitecture skos:broader :Architecture .

:PSACEnhancement owl:equivalentClass
    [a owl:Restriction ;
        owl:onProperty <http://fise.iks-project.eu/ontology/entity-reference> ;
        owl:someValuesFrom :PSACDocumentConcept ] .

:PSACDocument owl:equivalentClass
    [a owl:Restriction ;
        owl:onProperty :hasEnhancement ;
        owl:someValuesFrom
            [a owl:Restriction ;
                owl:onProperty <http://fise.iks-project.eu/ontology/entity-reference> ;
                owl:someValuesFrom :PSACDocumentConcept ] ] .

:PSACDocumentConcept owl:equivalentClass
    [a owl:Class ;
        owl:unionOf (
            :PSACBaseEntity
            [a owl:Restriction ;
                owl:onProperty skos:broaderTransitive ;
                owl:someValuesFrom :PSACBaseEntity] )
        ] .
